{
  "include": [
    "./rspack.config.ts"
  ],
  "compilerOptions": {
    "module": "Preserve",
    "moduleDetection": "force",
    "target": "esnext",
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "lib": [
      "ESNext"
    ],
    "paths": {
      "@tooling/*": [
        "./@tooling/*"
      ]
    }
  },
}